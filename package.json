{
  "name": "mauves-ui",
  "version": "0.0.0",
  "description": "Mauves UI component libraryy",
  "main": "./dist/mauves-ui.umd.js",
  "module": "./dist/mauves-ui.es.js",
  "types": "./dist/mauves-ui.es.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mauves-ui.es.js",
      "require": "./dist/mauves-ui.umd.js"
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "plop --plopfile ./.plop/plopfile.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "vite build",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "eslint src",
    "format": "yarn run format:prettier && yarn run format:eslint",
    "format:prettier": "prettier --write src",
    "format:eslint": "eslint --fix src",
    "prepare": "husky install"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/html": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "cssnano": "^5.1.14",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "plop": "^3.1.1",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vite-plugin-dts": "^1.6.6",
    "vite-plugin-solid": "^2.3.0"
  },
  "dependencies": {
    "@material/material-color-utilities": "^0.2.0",
    "solid-js": "^1.5.1"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ]
}
