{
  "name": "mauves-ui",
  "version": "0.0.0",
  "description": "Mauves UI component libraryy",
  "main": "./dist/mauves-ui.umd.js",
  "module": "./dist/mauves-ui.es.js",
  "types": "./dist/mauves-ui.es.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mauves-ui.es.js",
      "require": "./dist/mauves-ui.umd.js"
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "pnpm run lint:eslint",
    "lint:eslint": "eslint src",
    "format": "pnpm run format:prettier && pnpm run format:eslint",
    "format:prettier": "prettier --write src",
    "format:eslint": "eslint --fix src",
    "prepare": "husky install"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "prettier": "^2.7.1",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vite-plugin-dts": "^1.6.6",
    "vite-plugin-solid": "^2.3.0"
  },
  "dependencies": {
    "solid-js": "^1.5.1"
  }
}
